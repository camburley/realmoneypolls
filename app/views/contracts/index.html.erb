<div class="row">
  
<% @contracts.each do |contract| %>
  <div class="col-md-3">
      <div class="thumbnail">
        <img src="<%= contract.image %>">
        <div class="caption">
          <h3 class="title"> <%= contract.lastTradePrice %>  </h3>
          <p class="artist"> <%= contract.shortName %> </p>
        </div>
      </div>
    <% end %>
  </div>
 
  
</div>
